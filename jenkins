pipeline {
    agent {label 'JDK11 && MVN3 && NVM && PY3'} 
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Tajdarhussain110/openmrs-core.git' , branch : 'sprint-1' 
            }
        }
        stage('install') { 
            steps {
                sh 'openmrs-core'
            }
        }
    }
}